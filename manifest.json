{
  "manifest_version": 2,
  "name": "chrome-staticfont-cat",
  "description": "This extension for static file dev",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "view/setting.html"
  },
  "content_scripts": [
    {
      "matches": [
          "http://*/*"
      ],
      "js": [
        "lib/zepto.js",
        "src/content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "lib/zepto.js",
      "src/background.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "*://*/"
  ]
}