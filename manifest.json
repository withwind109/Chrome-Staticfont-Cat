{
  "manifest_version": 2,
  "name": "staticDev",
  "description": "This extension for static file dev",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  },
  "background": {
    "scripts": ["lib/jquery-2.1.4.min.js","src/background.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "*://*/"
  ]
}